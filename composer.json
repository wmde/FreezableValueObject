{
	"name": "wmde/freezable-value-object",
	"license": "GPL-2.0-or-later",
	"description": "PHP Trait for creating freezable value objects",
	"authors": [
		{
			"name": "Jeroen De Dauw",
			"email": "jeroendedauw@gmail.com",
			"homepage": "https://www.entropywins.wtf",
			"role": "Creator"
		}
	],
	"require": {
		"php": ">=8.2"
	},
	"require-dev": {
		"phpunit/phpunit": "~11.0.3",
		"wmde/fundraising-phpcs": "~10.0",
		"phpstan/phpstan": "~1.10.52"
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/wmde/fundraising-phpcs",
			"no-api": true
		}
	],
	"autoload": {
		"psr-4": {
			"WMDE\\FreezableValueObject\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"WMDE\\FreezableValueObject\\Tests\\": "tests/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.0.x-dev"
		}
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	}
}
